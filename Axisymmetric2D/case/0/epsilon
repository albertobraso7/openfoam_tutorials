FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.000765;

boundaryField
{
    tunnel
    {
        type            slip;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.000765;
        value           uniform 0.000765;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.000765;
    }
    ring
    {
        type            epsilonWallFunction;
        value           uniform 0;
    }
    wedge0
    {
        type            wedge;
    }
    wedge1
    {
        type            wedge;
    }
}


