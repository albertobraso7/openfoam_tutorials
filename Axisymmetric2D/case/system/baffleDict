FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createBafflesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

internalFacesOnly true;

baffles
{
    baffleFaces
    {
        type		faceZone;
        zoneName	plate;
    	flip		false;
	
        patches
        {
            master
            {
                name            plate;
                type            wall;
            }
            slave { ${..master} }
        }
    }
}

